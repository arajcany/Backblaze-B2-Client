{
  "name": "arajcany/backblaze-b2-client",
  "description": "Backblaze B2 Client to work with the native API.",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-curl": "*",
    "zaxbux/backblaze-b2-php": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "symfony/var-dumper": "^6.1",
    "zaxbux/flysystem-backblaze-b2": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "arajcany\\BackblazeB2Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "prefer-stable": true
}
